    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.1",
    "express-http-proxy": "^1.0.7",
    "html-element": "^2.2.0",
    "lodash.uniqby": "^4.7.0",
    "morgan": "^1.9.0",
